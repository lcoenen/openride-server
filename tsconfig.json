{

  "compilerOptions": {
    "target": "es2015",
    "sourceMap": true,
    "module": "commonjs",
    "removeComments": false,
    "noImplicitReturns": true,
    "noImplicitAny": true,
    "preserveConstEnums": true,
    "types": ["node", "restify", "chai"],
    "outDir": "built",
  },
  "exclude": [
    "logs",
    "node_modules"
  ]
}
